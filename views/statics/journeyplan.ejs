<h1>Plan your journey</h1>


<form id="journeydets">
  Travel from:
  <select id="origin" name="origin">
    <% stations.forEach((station) => { %>
      <option value="<%= station.icsCode %>"><%= station.name %></option>
    <% }) %>
  </select>
  to:
  <select id="destination" name="destination">
    <% stations.forEach((station) => { %>
      <option value="<%= station.icsCode %>"><%= station.name %></option>
    <% }) %>
  </select>

  <input type="submit" class="btn btn-success btn-sm" value="See your options">
</form>


<div class="card card-outline-info">
  <h3 class="card-header">Journey Results</h3>
  <div class="card-block" id="journeyresults">
  </div>
</div>
