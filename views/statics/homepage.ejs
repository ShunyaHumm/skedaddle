<h1>Homepage</h1>

<div class="row">
  <% users.forEach((user) => { %>
    <div class="col-md-4">
      <div class="card">
        <div class="card-block">
          <h4 class="card-title"><a href="/users/<%= user._id %>"><%= user.username %></a></h4>
          <p class="card-text">
            <%= user.email %>
            <%= user.password %>
          </p>
        </div>
      </div>
    </div>
  <% }) %>
</div>
